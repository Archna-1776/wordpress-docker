docker run --rm -v db_data:/volume -v D:\wordpress-docker\backup:/backup alpine tar -czf /backup/db_data.tar.gz /volume
docker run --rm -v wordpress_data:/volume -v D:\wordpress-docker\backup:/backup alpine tar -czf /backup/wordpress_data.tar.gz /volume

docker run --rm -v db_data:/volume -v D:\wordpress-docker\backup:/backup alpine sh -c "tar -xzf /backup/db_data.tar.gz -C /volume"
docker run --rm -v wordpress_data:/volume -v D:\wordpress-docker\backup:/backup alpine sh -c "tar -xzf /backup/wordpress_data.tar.gz -C /volume"

docker pull navin13121/wordpress-custom:latest
